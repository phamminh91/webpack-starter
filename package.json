  "lint-staged": {
    "linters": {
      "gitDir": "./",
      "*.{js,jsx}": [
        "prettier --single-quote --trailing-comma es5 --write",
        "eslint --fix --ext .js --ext .jsx",
        "git add"
      ],
      "*.{css,scss}": [
        "prettier --write",
        "stylelint --fix",
        "git add"
      ],
      "*.svg": [
        "sh ./build/svglinter/clean_svg_precommit.sh",
        "git add"
      ]
    }
  },
  "browserslist": [
    "ie > 9",
    "> 1%"
  ]
