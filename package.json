    "precss": "^2.0.0",
    "prettier": "^1.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "v8-lazy-parse-webpack-plugin": "^0.3.0",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.9.0",
